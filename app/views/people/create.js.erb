<% broadcast '/people/arrive' do %>
if ($("#users_online").find('tr:contains("<%= @user.email %>")').length == 0) {
  $("#users_online").append('<tr><td><%= @user.email %></td></tr>');
}
<% end %>
<% @expired_users.each do |user| %>
  $("#users_online").find('tr:contains("<%= user.email %>")').remove();
<% end %>